// Example program
#include <iostream>
#include <string>

int main()
{

    int i;
    int x;
    int a;
    int b;
    int c;
    int d;
    int e;
    int f;
    int g;
    int bnum;
    int eenum;
    int sum1;
    //
    //
    //
    //
    //
    for(i = 1; i<=10; i++)
    {
        std::cout << i << "  ";
    }
    
    
    //
    std::cout << std::endl;
    //
    
    
    for (x=-10; x<=10; x++)
    {
        std::cout << x << "  ";
    }
    
    
    //
    std::cout << std::endl;
    //
    
    
    for(a = 100; a<=1000; a = a + 100)
    {
        std::cout << a << "  ";
    }
    //
    std::cout << std::endl;
    //
    for (b = 1; b<=100; b = b+2)
    {
        
        std::cout << b << "  ";
        
    }
    
    
    
    //
    std::cout << std::endl;
    std::cout << std::endl;
    std::cout << std::endl;
    //
    
    
    
    
    for (c = 1; c<=8; c++)
    {
        std::cout << "5 * " << c << " = " << 5*c << std::endl;
    }
    
    
    
    //
    //
    //
    
    
    

    int sum;
    for (d = 25; d<=125; d=d+1)
    {
        e = e + d;
    }
    std::cout << "The sum of 25 to 125 is " << e;
    std::cout << "\n";

    
    
    //
    std::cout << std::endl;
    std::cout << std::endl;
    std::cout << std::endl;
    //
    
    
    

    for (f = -5; f<=10; f=f+1)
    {
        g = g + f;
    }
    std::cout << "The sum of -5 to 10 is " << g;
    std::cout << "\n";
    
    
    //
    std::cout << std::endl;
    std::cout << std::endl;
    std::cout << std::endl;
    //
    
    
    
    std::cout << "Enter the beginning number: ";
    std::cin>>bnum;
    std::cout << "Enter the ending number: ";
    std::cin>>eenum;
    
    for (e = bnum; e<=eenum; e=e+1)
    {
        std::cout << e << " ";
    
 
    }
    
}
